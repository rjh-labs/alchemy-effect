    "effect": "catalog:",
    "esbuild": "^0.25.12",
    "fast-xml-parser": "^5.2.5",
    "itty-aws": "0.4.1",
    "jszip": "^3.10.1",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@effect/platform-node": "catalog:",
    "@effect/platform": "catalog:",
    "@types/bun": "latest",
    "@types/node": "latest"
